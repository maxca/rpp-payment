{
    "name": "samarkchaisanguan/rpp-payment",
    "description": "for rpp payment integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mark",
            "email": "Samark.Cha@ascendcorp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "arcanedev/support": "^4.5",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "guzzlehttp/guzzle": ">=5.0.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-0": {
            "Samark\\RppPayment\\": "src/"
        },
        "files": ["src\\constant.php"]
    },
    "autoload-dev": {
        "psr-0": {
            "Samark\\RppPayment\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Samark\\RppPayment\\Providers\\RppServiceProvider"
            ]
        }
    }
}
